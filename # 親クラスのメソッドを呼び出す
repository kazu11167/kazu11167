class Greeting:
    def __init__(self):
        self.msg = "hello"
        self.target = "paiza"

    def say_hello(self):
        print(self.msg + " " + self.target)

class Hello(Greeting):
    def say_hello(self):

        print("YEAH YEAH YEAH!")

player1 = Greeting()
player1.say_hello()
player = Hello()
player.say_hello()
